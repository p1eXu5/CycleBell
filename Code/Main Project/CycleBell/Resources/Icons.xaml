    <Color x:Key="col_Background">#FFFFFF</Color>
    <Color x:Key="col_Foreground">#000000</Color>
    <Color x:Key="col_Tip">#939393</Color>
    <Color x:Key="col_NoSetTime">#E0E0E0</Color>
    <Color x:Key="col_UncheckedStroke">#939393</Color>
    <Color x:Key="col_IconGreyColor">#C8C8C8</Color>
    <Color x:Key="col_CheckedStroke">#00000000</Color>

    <SolidColorBrush x:Key="BackgroundBrush" Color="{DynamicResource col_Background}" />
    <SolidColorBrush x:Key="brush_Foreground" Color="{DynamicResource col_Foreground}" />
    <SolidColorBrush x:Key="brush_Tip" Color="{DynamicResource col_Tip}" />
    <SolidColorBrush x:Key="brush_NoSetTime" Color="{DynamicResource col_NoSetTime}" />
    <SolidColorBrush x:Key="brush_UncheckedStroke" Color="{DynamicResource col_UncheckedStroke}" />
    <SolidColorBrush x:Key="brush_Marker" Color="#008900" />

    <LinearGradientBrush x:Key="brush_CheckedIcon" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#7F7F7F" />
        <GradientStop Offset="1" Color="{DynamicResource col_Foreground}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="brush_UncheckedIcon" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="{DynamicResource col_Background}" />
        <GradientStop Offset="1" Color="{DynamicResource col_IconGreyColor}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="MenuLineBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="{DynamicResource col_IconGreyColor}" />
        <GradientStop Offset="1" Color="{DynamicResource col_Background}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="StatusLineBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="{DynamicResource col_Background}" />
        <GradientStop Offset="1" Color="{DynamicResource col_IconGreyColor}" />
    </LinearGradientBrush>

</ResourceDictionary>